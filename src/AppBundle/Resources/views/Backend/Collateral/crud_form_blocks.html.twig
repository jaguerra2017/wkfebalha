<div data-ng-if="model.formActiveView == 'blocks'" class="row" id="collateral_blocks">
    <div data-ng-if="model.createAction == true" style="display:block;text-align: center; color:#93a2a9; padding: 20px;">
        Primero debe de <strong>Guardar</strong> para poder agregar Bloques a esta Colateral.
    </div>
    <div data-ng-if="model.createAction == false && model.selectedCollateral.id != undefined">
        <block-selector elementid="model.selectedCollateral.id" elementtype="collateral"></block-selector>
    </div>
</div>