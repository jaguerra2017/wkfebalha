<div data-ng-if="model.formActiveView == 'blocks'" class="row" id="partner_blocks">
    <div data-ng-if="model.createAction == true" style="display:block;text-align: center; color:#93a2a9; padding: 20px;">
        Primero debe de <strong>Guardar</strong> para poder agregar Bloques a este Socio.
    </div>
    <div data-ng-if="model.createAction == false && model.selectedPartner.id != undefined">
        <block-selector elementid="model.selectedPartner.id" elementtype="partner"></block-selector>
    </div>
</div>